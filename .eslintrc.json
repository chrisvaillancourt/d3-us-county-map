{
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "warn",
    "quotes": [
      "error",
      "backtick"
    ],
    "comma-dangle": [
      "error",
      "always"
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "always",
        "asyncArrow": "always"
      }
    ],
    "space-before-blocks": [
      "error",
      "always"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-multi-assign": "off",
    "no-fallthrough": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  }
}